{
  "name": "knownet",
  "version": "0.1.0",
  "description": "A personal knowledge network for tracking beliefs, theories, and conclusions",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon",
    "dev:tsx": "tsx watch src/cli/index.ts",
    "start": "node dist/cli/index.js",
    "api": "node dist/api/server.js",
    "api:dev": "nodemon --watch src --ext ts --exec tsx src/api/server.ts",
    "api:build": "npm run build && npm run api",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "knowledge",
    "graph",
    "beliefs",
    "reasoning"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "@inquirer/prompts": "^7.7.1",
    "axios": "^1.11.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "figlet": "^1.8.2",
    "https-proxy-agent": "^7.0.6",
    "inquirer": "^12.8.2",
    "ora": "^5.4.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/figlet": "^1.7.0",
    "@types/inquirer": "^9.0.8",
    "@types/node": "^20.19.9",
    "@types/uuid": "^9.0.8",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  }
}
